{
  "name": "eleventy-domdiff-postcss",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "concurrently \"npm run start:eleventy\" \"npm run start:tailwind\"",
    "start:eleventy": "eleventy --serve",
    "start:tailwind": "tailwindcss --watch --output _site/style.css",
    "build": "eleventy",
    "clean": "rm -rf _site"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.0-canary.4",
    "autoprefixer": "^10.4.4",
    "concurrently": "^7.0.0",
    "postcss": "^8.4.12",
    "tailwindcss": "^3.0.23"
  }
}
